// ============================================================================
public class Vec2
  private vec2 m_value
  
  // --------------------------------------------------------------------------
  construct()
    m_value = ZERO2
  
  // --------------------------------------------------------------------------
  construct(vec2 value)
    m_value = value
  
  // --------------------------------------------------------------------------
  function getValue() returns vec2
    return m_value
    
  // --------------------------------------------------------------------------
  function setValue(vec2 value)
    m_value = value