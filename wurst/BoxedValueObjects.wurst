package BoxedValueObjects

// ============================================================================
public class String
  private string m_value
  
  // --------------------------------------------------------------------------
  construct()
    m_value = ""
  
  // --------------------------------------------------------------------------
  construct(string value)
    m_value = value

  // --------------------------------------------------------------------------
  function getValue() returns string
    return m_value

  // --------------------------------------------------------------------------
  function setValue(string value)
    m_value = value

// ============================================================================
public class Integer
  private int m_value
  
  // --------------------------------------------------------------------------
  construct()
    m_value = 0
  
  // --------------------------------------------------------------------------
  construct(int value)
    m_value = value
    
  // --------------------------------------------------------------------------
  function getValue() returns int
    return m_value
    
  // --------------------------------------------------------------------------
  function setValue(int value)
    m_value = value

// ============================================================================
public class Real
  private real m_value
  
  // --------------------------------------------------------------------------
  construct()
    m_value = 0.
  
  // --------------------------------------------------------------------------
  construct(real value)
    m_value = value
  
  // --------------------------------------------------------------------------
  function getValue() returns real
    return m_value
    
  // --------------------------------------------------------------------------
  function setValue(real value)
    m_value = value

// ============================================================================
